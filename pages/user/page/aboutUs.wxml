<!--pages/user/page/aboutUs.wxml-->
<view class='usInfoPages'>
  <view class='infoItems'>
    <view class='info-item' >
        <text class="item-center">美思健身-专注健身30年
        </text>            
    </view>    
    <view class='info-item'>
        <text class="item-left">咨询电话：
        </text>
        <text class="item-right" selectable="true">{{usTel}}
        </text> 
    </view>   
     <view class='info-item'>
        <text class="item-left">微信客服：
        </text>
        <text class="item-right" selectable="true">{{usWeixin}}
        </text> 
    </view>  
     <view class='info-item'>
        <text class="item-left">官方网站：
        </text>
        <text class="item-right" selectable="true">{{usWeb}}
        </text>  
    </view>  
     <view class='info-item'>
        <text class="item-left">地址：
        </text> 
        <text class="item-right" selectable="true">{{usAddress}}
        </text> 
    </view>  
  </view>
</view>
