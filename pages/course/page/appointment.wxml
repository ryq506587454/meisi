<!--pages/course/page/appointment.wxml-->
<view class='appoPage'>
  <view class='page-title'>
    <view class='page-title-top'>
      <text>预约界面
      </text>
    </view>
    <view class='page-title-bottom'>      
      <text class='iconfont icon-shengyin' decode='true'>&nbsp;&nbsp;点击返回键重新选择
      </text>
    </view>
  </view>
  <view class='content'>
    <view class='courseInfo'>
      <view class='infoItam'>
      <text class='left'>课程编号:
      </text>
      <text class='right'>{{courseInfo[0].courseId}}
      </text >
      </view>
      <view class='infoItam'>
       <text class='left'>课程名称:
      </text>
      <text class='right'>{{courseInfo[0].courseName}}
      </text >
      </view>
      <view class='infoItam'>
       <text class='left'>选课人数:
      </text>
      <text class='right'>{{courseInfo[0].user.length}} / {{courseInfo[0].totalNumber}}
      </text >
      </view>
      <view class='infoItam'>
       <text class='left'>开课时间:
      </text>
      <text class='right'>{{courseInfo[0].startDate}}
      </text >
      </view>
    </view>
  </view>
</view>

